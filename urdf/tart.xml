<robot name="tart_robot">
	
	
	<link name="base_link">
		<visual>
		    <origin xyz="0 0.15 0.01" rpy="0 0 0"/>
			<geometry>
				<box size="0.35 0.35 0.02"/>
			</geometry>
			<material name = "grey1">
	       		<color rgba="0.5 0.5 0.5 1.0"/>
			</material>
	</visual>
	</link>

	
	<joint name="base_joint" type="fixed">
    	<parent link="base_link"/>
    	<child link="base_motor_holder"/>
    	<origin xyz="0 0 0.02" rpy="0 0 0"/>
    	<axis xyz="0 0 1"/>
	</joint>
	
		
	<link name="base_motor_holder">
		<visual>
		    <origin xyz="0 0 0.025" rpy="0 0 0"/>
			<geometry>
				<box size="0.05 0.05 0.05"/>
			</geometry>
			<material name = "white1">
	       		<color rgba="1 1 1 1.0"/>
			</material>
	</visual>
	</link>
	
	<joint name="motor_1" type="continuous">
    	<parent link="base_motor_holder"/>
    	<child link="rotating_plate"/>
    	<origin xyz="0 0 0.055" rpy="0 0 0"/>
    	<axis xyz="0 0 1"/>
	</joint>
	
		
	<link name="rotating_plate">
		<visual>
		    <origin xyz="0 0 0.005" rpy="0 0 0"/>
			<geometry>
				<box size="0.07 0.07 0.01"/>
			</geometry>
			<material name = "white1">
	       		<color rgba="1 1 1 1.0"/>
			</material>
	</visual>
	</link>
	
	<joint name="rotating_plate_joint" type="fixed">
    	<parent link="rotating_plate"/>
    	<child link="plate_link"/>
    	<origin xyz="0 0 0.01" rpy="0 0 0"/>
    	<axis xyz="0 0 1"/>
	</joint>
	
		
	<link name="plate_link">
		<visual>
		    <origin xyz="0 0 0.015" rpy="0 0 0"/>
			<geometry>
				<box size="0.005 0.03 0.03"/>
			</geometry>
			<material name = "white1">
	       		<color rgba="1 1 1 1.0"/>
			</material>
	</visual>
	</link>
	
	<joint name="motor_2" type="continuous">
    	<parent link="plate_link"/>
    	<child link="main_link_1"/>
    	<origin xyz="0.005 0 0.015" rpy="0 0 0"/>
    	<axis xyz="1 0 0"/>
	</joint>
	
		
	<link name="main_link_1">
		<visual>
		    <origin xyz="0 0 0.12" rpy="0 0 0"/>
			<geometry>
				<box size="0.01 0.01 0.24"/>
			</geometry>
			<material name = "blue1">
	       		<color rgba="0.2 0.2 1 1.0"/>
			</material>
	</visual>
	</link>
	
	<joint name="motor_3" type="continuous">
    	<parent link="main_link_1"/>
    	<child link="main_link_2"/>
    	<origin xyz="0.005 0 0.2" rpy="0 0 0"/>
    	<axis xyz="1 0 0"/>
	</joint>
	
		
	<link name="main_link_2">
		<visual>
		    <origin xyz="0 0 0.08" rpy="0 0 0"/>
			<geometry>
				<box size="0.01 0.01 0.20"/>
			</geometry>
			<material name = "brown1">
	       		<color rgba="0.4 0 0 1.0"/>
			</material>
	</visual>
	</link>
   
</robot>